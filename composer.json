{
    "name": "jamesgordo/laravel-japan-postalcode",
    "description": "A package to add endpoints to search Japan addresses using a Postal Code on any existing laravel application.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "JamesGordo\\JapanPostalCode\\": "src/app/",
            "JamesGordo\\JapanPostalCode\\Database\\": "src/database/",
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "James Gordo"
        }
    ],
    "require": {
        "jamesgordo/php-csv-parser": "^1.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamesGordo\\JapanPostalCode\\Providers\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.25",
        "pestphp/pest": "^2.17"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
